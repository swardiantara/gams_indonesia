<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h6 class="card-title">{{{title}}}</h6>
                <div class="table-responsive">
                    <table id="tb_memberlist" class="table table-hover text-center">
                        <thead>
                            <tr>
                                <th class="text-left">Nama</th>
                                <th>License</th>
                                <th>Team Member</th>
                                <th>Rekening Bank</th>
                                <th>No. Rekening</th>
                                <th>Commission</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each data}}
                            <tr>
                                <th class="text-left"><a href="/profile/member/{{_id}}">{{fullName}}</a></th>
                                <td>{{license}}</td>
                                <td><a href="/team-member/{{_id}}"> {{downline.length}}</a></td>
                                <td>{{bankAccount}}</td>
                                <td>{{bankNumber}}</td>
                                <td><a href="/commission/{{_id}}">{{formatprice komisi}}</a></td>
                                <td>
                                    {{#each license}}
                                    {{this.name}}
                                    {{/each}}
                                </td>
                                {{!-- <td><a href="/membership/delete/{{_id}}" class="text-danger">View Team</td>--}}
                                <td><a href="/membership/delete/{{_id}}" class="text-danger">Hapus</td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>