        <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordionExample">
            <div class="card-body">
                <h6 class="card-title">Komisi {{data.fullName}}</h6>
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>Waktu</th>
                                <th>Tipe</th>
                                {{!-- <th>Detail</th> --}}
                                <th>Status</th>
                                <th>Nominal</th>
                                {{!-- <th colspan="">Total</th> --}}
                            </tr>
                        </thead>
                        <tbody>
                            {{#each data.commission}}
                            <tr>
                                <th>{{this.createdAt}}</th>
                                <th>{{this.type}}</th>
                                {{!-- <td>{{this.order}}</td> --}}
                                <td>{{#ifCond this.status 'not_paid'}}Belum dibayar{{/ifCond}}
                                    {{#ifCond this.status 'paid'}} Sudah dibayar{{/ifCond}}</td>
                                <td>{{formatprice this.jumlah}}</td>
                                {{!-- <td>{{this.platform}}</td> --}}
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="card-footer row justify-content-between mx-3">
                <span>Total Komisi : {{formatprice total}}</span>
                {{#if data.commission}}
                <button id="resetKomisi" class="btn btn-success" data-id="{{data._id}}">Lunaskan</button>
                {{else}}
                <span class="d-inline-block d-flex justify-content-center" tabindex="0" data-toggle="tooltip"
                    title="Tidak ada komisi!">
                    <button class="btn btn-primary" style="pointer-events: none;" type="button"
                        disabled>Lunaskan</button>
                </span>
                {{!-- <button id="resetKomisi" class="btn btn-success" data-id="{{data._id}}" disabled>Lunaskan</button> --}}
                {{/if}}
            </div>
        </div>
    </div>
